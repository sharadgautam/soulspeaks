nexport default {\r\n  name: 'ManageDids',\r\n  data() {\r\n    return {\r\n      ownedDids: [],\r\n      trunks: [],\r\n      selectedDid: '',\r\n      selectedTrunk: '',\r\n      assignments: [],\r\n      message: ''\r\n    };\r\n  },\r\n  methods: {\r\n    assignTrunk() {\r\n      if (this.selectedDid && this.selectedTrunk) {\r\n        this.assignments.push({ did: this.selectedDid, trunk: this.selectedTrunk });\r\n        this.message = `Assigned trunk ${this.selectedTrunk} to DID ${this.selectedDid}`;\r\n        this.selectedDid = '';\r\n        this.selectedTrunk = '';\r\n      } else {\r\n        this.message = 'Please select both a DID and a trunk.';\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.manage-dids-container {\r\n  max-width: 600px;\r\n  margin: 60px auto;\r\n  padding: 2em;\r\n  border: 1px solid #ccc;\r\n  border-radius: 8px;\r\n  text-align: center;\r\n}\r\n.message {\r\n  color: green;\r\n  margin-top: 1em;\r\n}\r\n</style> "]}]}